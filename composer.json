{
    "name": "slince/cakephp-permission",
    "description": "The library provides a flexible way to add role-based access control management to CakePHP 3.x",
    "keywords": ["cakephp", "permission","RBAC", "role based", "security", "manage permission", "manage roles", "manage users"],
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Tao",
            "email": "taosikai@yeah.net"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "cakephp/cakephp": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Slince\\CakePermission\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slince\\CakePermission\\Tests\\": "./tests"
        }
    }
}
